{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "main": "./src/main/main.js",
  "dependencies": {
    "antd": "3.0.3",
    "electron": "1.8.1",
    "mobx": "3.4.1",
    "mobx-react": "4.3.5",
    "mobx-react-router": "4.0.1",
    "mobx-state-tree": "1.3.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-loadable": "5.3.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "server": "react-app-rewired start",
    "build-render": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "build-main": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --config ./config-main.js --progress --profile --colors",
    "client": "cross-env HOT=1 NODE_ENV=development ./node_modules/.bin/electron ./src/main/main.js",
    "dev": "concurrently --kill-others \"yarn run server\" \"wait-on -d 15000 -t 20000 http://localhost:3000 && yarn run client\""
  },
  "devDependencies": {
    "babel-plugin-import": "1.6.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-register": "6.26.0",
    "concurrently": "^3.5.1",
    "cross-env": "5.1.3",
    "electron-builder": "19.49.2",
    "electron-updater": "2.17.6",
    "react-app-rewire-eslint": "0.2.3",
    "react-app-rewire-less": "2.1.0",
    "react-app-rewired": "1.4.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "wait-on": "^2.0.2"
  }
}